{
  "name": "misthin-tool",
  "version": "0.0.8",
  "author": "bwrong<ibwrong@foxmail.com>",
  "license": "MIT",
  "keywords": [
    "misthin",
    "cli",
    "vue",
    "react",
    "template",
    "deploy",
    "electron"
  ],
  "description": "",
  "scripts": {
    "serve": "vue-cli-service electron:serve",
    "build": "vue-cli-service electron:build",
    "build:w": "npm run build -- --win",
    "build:l": "npm run build -- --linux",
    "build:all": "npm run build -- -mwl",
    "generate-icons": "electron-icon-builder --input=./build/logo.png --output=build --flatten",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "publish": "npm run build:all -- -p onTag",
    "patch": "npm version patch && git push origin master && git push origin --tags",
    "minor": "npm version minor && git push origin master && git push origin --tags",
    "major": "npm version major && git push origin master && git push origin --tags",
    "release": "npm run build -- -mwl"
  },
  "main": "background.js",
  "dependencies": {
    "ant-design-vue": "^2.0.0-rc.8",
    "core-js": "^3.8.3",
    "dayjs": "^1.10.3",
    "download-git-repo": "^3.0.2",
    "electron-updater": "^4.3.5",
    "lowdb": "^1.0.0",
    "node-ssh": "^11.1.1",
    "nprogress": "^0.2.0",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0-0",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/fs-extra": "^9.0.6",
    "@types/lowdb": "^1.0.9",
    "@types/node": "^14.14.22",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "antd-dayjs-webpack-plugin": "^1.0.6",
    "babel-plugin-import": "^1.13.3",
    "cz-conventional-changelog": "^3.3.0",
    "electron": "^11.2.0",
    "electron-devtools-installer": "^3.1.0",
    "electron-icon-builder": "^2.0.1",
    "eslint": "^7.18.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^7.5.0",
    "less": "^4.1.0",
    "less-loader": "^6.2.0",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "standard-version": "^9.1.0",
    "typescript": "^4.1.3",
    "vue-cli-plugin-electron-builder": "^2.0.0-rc.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "standard-version": {
    "skip": {
      "commit": false
    }
  }
}
