  "description": "",
  "scripts": {
    "serve": "vue-cli-service electron:serve",
    "build": "vue-cli-service electron:build",
    "build:win": "vue-cli-service electron:build -- --win",
    "build:mac": "vue-cli-service electron:build -- --mac",
    "build:all": "vue-cli-service electron:build -- -mwl",
    "generate-icons": "electron-icon-builder --input=./public/logo.png --output=build --flatten",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "publish": "export GH_TOKEN=c33017a12ba6f9ac1dd3495efbae225750644acc && npm run build:all -- -p always"
  },
  "main": "background.js",
  "dependencies": {
    "ant-design-vue": "^2.0.0-rc.8",
    "core-js": "^3.8.2",
    "dayjs": "^1.10.3",
    "electron-updater": "^4.3.5",
    "lowdb": "^1.0.0",
    "nprogress": "^0.2.0",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0-0",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/fs-extra": "^9.0.6",
    "@types/lowdb": "^1.0.9",
    "@types/node": "^14.14.20",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "babel-plugin-import": "^1.13.3",
    "cz-conventional-changelog": "^3.3.0",
    "electron": "^9.4.0",
    "electron-devtools-installer": "^3.1.0",
    "electron-icon-builder": "^2.0.1",
    "eslint": "^7.17.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^7.0.0-0",
    "less": "^4.1.0",
    "less-loader": "^7.2.1",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "standard-version": "^9.1.0",
    "typescript": "^4.1.3",
    "vue-cli-plugin-electron-builder": "^2.0.0-rc.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "standard-version": {
    "skip": {
      "commit": false
    }
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
